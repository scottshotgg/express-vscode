{
    "name": "expr",
    "displayName": "expr Assembly Language",
    "description": "Syntax highlighting for expr assembly instructions",
    "version": "1.0.5",
    "publisher": "nickymarino2",
    "author": {
        "name": "Nicky Marion",
        "email": "nicky.s.marino@gmail.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/heztet/expr-vscode"
    },
    "bugs": {
        "url": "https://github.com/heztet/expr-vscode/issues",
        "email": "nicky.s.marino@gmail.com"
    },
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "expr", "expr", "Assembly", "syntax-highlighter", "languages" 
    ],
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "contributes": {
        "languages": [
            {
                "id": "expr",
                "aliases": [
                    "expr",
                    "expr"
                ],
                "extensions": [
                    ".asm",
                    ".inc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "expr",
                "scopeName": "source.expr",
                "path": "./syntaxes/expr.tmLanguage.json"
            }
        ]
    }
}